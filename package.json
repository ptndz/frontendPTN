    "autosize": "^6.0.1",
    "axios": "^1.3.4",
    "cookies-next": "^2.1.1",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "graphql": "^16.6.0",
    "graphql-request": "^5.2.0",
    "moment": "^2.29.4",
    "next": "13.2.4",
    "next-pwa": "^5.6.0",
    "next-seo": "^5.15.0",
    "nprogress": "^0.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.7",
    "react-icons": "^4.8.0",
